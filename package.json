{
  "name": "@fireenjin/components",
  "namespace": "fireenjin",
  "version": "2.0.0",
  "private": false,
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/components.d.ts",
  "collection:main": "dist/collection/index.js",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "watch": {
    "build": {
      "patterns": [
        "src",
        "overrides"
      ],
      "extensions": "js,jsx,tsx,ts,css",
      "quiet": true,
      "legacyWatch": true,
      "delay": 2500,
      "runOnChangeOnly": false
    }
  },
  "scripts": {
    "build": "mitosis build",
    "start": "npm run watch",
    "env": "fireenjin env",
    "presets": "fireenjin presets",
    "presets:watch": "fireenjin presets watch",
    "watch": "npm-watch"
  },
  "dependencies": {
    "@builder.io/mitosis": "^0.0.71",
    "@builder.io/mitosis-cli": "^0.0.31",
    "@fireenjin/sdk": "^1.2.63",
    "@stripe/stripe-js": "^1.39.0",
    "chart.js": "^3.9.1",
    "cleave.js": "^1.6.0",
    "clipboard": "^2.0.11",
    "date-fns": "^2.29.3",
    "debounce-decorator": "^1.0.6",
    "google-maps": "^4.3.3",
    "jkanban": "^1.3.1",
    "json-formatter-js": "^2.3.4",
    "node-fetch": "^2.6.1",
    "vanilla-jsoneditor": "^0.7.9"
  },
  "devDependencies": {
    "@builder.io/eslint-plugin-mitosis": "^0.0.10",
    "@fireenjin/cli": "^1.1.5",
    "@types/cleave.js": "^1.4.7",
    "@types/jest": "^27.4.0",
    "@types/node-fetch": "^2.5.12",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^8.17.0",
    "husky": "^8.0.1",
    "jest": "^27.5.1",
    "jest-fetch-mock": "^3.0.3",
    "npm-watch": "^0.7.0",
    "prettier": "^2.5.0",
    "ts-jest": "^27.1.3",
    "typescript": "^4.8.4"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "description": "FireEnjin Components - UI for the Next Generation",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FireEnjin/Components.git"
  },
  "keywords": [
    "firebase",
    "full-stack",
    "typescript"
  ],
  "author": "Madness Labs",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FireEnjin/Components/issues"
  },
  "homepage": "https://github.com/FireEnjin/Components#readme"
}