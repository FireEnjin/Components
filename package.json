{
  "name": "@fireenjin/components",
  "namespace": "fireenjin",
  "version": "2.0.0",
  "private": false,
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/components.d.ts",
  "collection:main": "dist/collection/index.js",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "watch": {
    "build": {
      "patterns": [
        "src",
        "overrides"
      ],
      "extensions": "js,jsx,tsx,ts,css",
      "quiet": true,
      "legacyWatch": true,
      "delay": 2500,
      "runOnChangeOnly": false
    },
    "copy:qwik": {
      "patterns": [
        "output/qwik"
      ],
      "extensions": "js,jsx,tsx,ts,css",
      "quiet": true,
      "legacyWatch": true,
      "delay": 2500,
      "runOnChangeOnly": false
    }
  },
  "scripts": {
    "build": "mitosis build",
    "build:qwik": "npm run build && cd packages/qwik && npm run build",
    "build:qwik:release": "npm run build && npm run env release && cd packages/qwik && npm run build.release",
    "build:stencil": "npm run task:stencil && npm run copy:stencil && npm run copy:stencil:overrides && cd packages/stencil && npm run build",
    "b:stencil": "npm run copy:stencil:overrides && cd packages/stencil && npm run build",
    "compile:stencil": "mitosis compile -t stencil",
    "task:stencil": "node tasks/stencil.js",
    "copy": "fireenjin copy",
    "copy:css:qwik": "npm run copy ./output/fireenjin.css ./packages/qwik/src/fireenjin.css",
    "copy:qwik": "npm run copy ./output/qwik/src/components/ ./packages/qwik/src/components",
    "copy:stencil": "npm run copy ./output/stencil/src/components/ ./packages/stencil/src/components",
    "copy:stencil:overrides": "npm run copy ./overrides/stencil/src/components/ ./packages/stencil/src/components",
    "g": "fireenjin generate",
    "env": "fireenjin env",
    "pollen": "pollen",
    "postbuild": "pollen",
    "presets": "fireenjin presets",
    "presets:watch": "fireenjin presets watch",
    "publish:qwik": "npm run build:qwik:release && cd packages/qwik && npm publish",
    "start": "run-p watch start:qwik",
    "start:qwik": "cd packages/qwik && npm run start",
    "start:stencil": "cd packages/stencil && npm run start",
    "watch": "npm-watch"
  },
  "dependencies": {
    "@builder.io/mitosis": "^0.0.92",
    "@builder.io/mitosis-cli": "^0.0.51",
    "@fireenjin/sdk": "^1.3.3",
    "@stripe/stripe-js": "^1.47.0",
    "chart.js": "^4.2.1",
    "cleave.js": "^1.6.0",
    "clipboard": "^2.0.11",
    "date-fns": "^2.29.3",
    "debounce-decorator": "^1.0.6",
    "google-maps": "^4.3.3",
    "jkanban": "^1.3.1",
    "json-formatter-js": "^2.3.4",
    "node-fetch": "^3.3.0",
    "vanilla-jsoneditor": "^0.15.1"
  },
  "devDependencies": {
    "@builder.io/eslint-plugin-mitosis": "^0.0.14",
    "@fireenjin/cli": "^1.1.5",
    "@types/cleave.js": "^1.4.7",
    "@types/jest": "^29.4.0",
    "@types/node-fetch": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint": "^8.35.0",
    "glob": "^9.1.2",
    "husky": "^8.0.3",
    "jest": "^29.4.3",
    "jest-fetch-mock": "^3.0.3",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.11.0",
    "pollen-css": "^4.6.2",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "description": "FireEnjin Components - UI for the Next Generation",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FireEnjin/Components.git"
  },
  "keywords": [
    "firebase",
    "full-stack",
    "typescript"
  ],
  "author": "Madness Labs",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FireEnjin/Components/issues"
  },
  "homepage": "https://github.com/FireEnjin/Components#readme"
}